{
  "stepDefinitions": [
    "cypress/e2e/**/*.steps.js"
  ],
  "html": {
    "enabled": true,
    "output": "cypress/reports/new/[timestamp]/html/cucumber-report.html"
  },
  "json": {
    "enabled": true,
    "output": "cypress/reports/new/[timestamp]/json/cucumber-report.json"
  },
  "messages": {
    "enabled": true,
    "output": "cypress/reports/new/[timestamp]/json/cucumber-report.ndjson"
  }
} 